#include <bits/stdc++.h>
using namespace std;
 
int main() {
	// your code goes here
    int t;
    cin >> t;
    
    while(t--){
        int n;
        cin >> n;
        
        int ans = 1;
        
        for(int i =2; i <= n/2; i++){
            if(2*i <= n){
                ans = i;
            }
        }
        cout << ans << endl;
    }
    return 0;
}
